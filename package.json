{
  "name": "@bbortt/owl",
  "description": "alternative GIT hook - MIT licensed.",
  "version": "1.0.0-0",
  "license": "MIT",
  "bin": {
    "owl": "src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbortt/owl.git"
  },
  "bugs": {
    "url": "https://github.com/bbortt/owl/issues"
  },
  "files": [
    "/lib",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "_postinstall": "node src install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prettier": "prettier {,src/**/*}.{js,json,md}",
    "prettier:fix": "npm run prettier -- -w"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "engines": {
    "node": ">= 14"
  },
  "keywords": [
    "git",
    "hook",
    "hooks"
  ]
}
